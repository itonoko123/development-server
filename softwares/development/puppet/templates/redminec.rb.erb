#!ruby
require "rubygems"
require "redmine_client"

RedmineClient::Base.configure do
  self.site = "http://<%= redmine %>:<%= redmine_port %>"
  self.user = "admin"
  self.password = "admin"
end

project = RedmineClient::Project.new(
	:name => "testproject",
	:identifier => "test",
	:description => "test"
)

if project.save
  puts project.id
else
  puts "error"
end
