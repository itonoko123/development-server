[gerrit]
	basePath = git
	canonicalWebUrl = http://<%= gerrit %>:<%= gerrit_port %>/
[database]
	type = H2
	database = db/ReviewDB
[auth]
	type = OPENID
[sendemail]
	smtpServer = localhost
[container]
	user = root
	javaHome = /usr/lib/jvm/java-6-openjdk-i386/jre
[sshd]
	listenAddress = *:29418
[httpd]
	listenUrl = http://*:<%= gerrit_port %>/
[cache]
	directory = cache
